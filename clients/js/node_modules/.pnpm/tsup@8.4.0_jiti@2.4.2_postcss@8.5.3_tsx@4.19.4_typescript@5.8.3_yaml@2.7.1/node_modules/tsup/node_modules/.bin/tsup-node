#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/itaismith/Developer/chroma/clients/new-js/node_modules/.pnpm/tsup@8.4.0_jiti@2.4.2_postcss@8.5.3_tsx@4.19.4_typescript@5.8.3_yaml@2.7.1/node_modules/tsup/dist/node_modules:/Users/itaismith/Developer/chroma/clients/new-js/node_modules/.pnpm/tsup@8.4.0_jiti@2.4.2_postcss@8.5.3_tsx@4.19.4_typescript@5.8.3_yaml@2.7.1/node_modules/tsup/node_modules:/Users/itaismith/Developer/chroma/clients/new-js/node_modules/.pnpm/tsup@8.4.0_jiti@2.4.2_postcss@8.5.3_tsx@4.19.4_typescript@5.8.3_yaml@2.7.1/node_modules:/Users/itaismith/Developer/chroma/clients/new-js/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/itaismith/Developer/chroma/clients/new-js/node_modules/.pnpm/tsup@8.4.0_jiti@2.4.2_postcss@8.5.3_tsx@4.19.4_typescript@5.8.3_yaml@2.7.1/node_modules/tsup/dist/node_modules:/Users/itaismith/Developer/chroma/clients/new-js/node_modules/.pnpm/tsup@8.4.0_jiti@2.4.2_postcss@8.5.3_tsx@4.19.4_typescript@5.8.3_yaml@2.7.1/node_modules/tsup/node_modules:/Users/itaismith/Developer/chroma/clients/new-js/node_modules/.pnpm/tsup@8.4.0_jiti@2.4.2_postcss@8.5.3_tsx@4.19.4_typescript@5.8.3_yaml@2.7.1/node_modules:/Users/itaismith/Developer/chroma/clients/new-js/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli-node.js" "$@"
else
  exec node  "$basedir/../../dist/cli-node.js" "$@"
fi
