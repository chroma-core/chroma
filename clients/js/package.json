{
  "name": "chromadb",
  "version": "2.0.0",
  "description": "A JavaScript interface for chroma",
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.22.11",
    "@openapi-generator-plus/typescript-fetch-client-generator": "^1.5.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.5.6",
    "@visheratin/tokenizers-node": "0.1.5",
    "@visheratin/web-ai-node": "^1.3.1",
    "@xenova/transformers": "^2.5.3",
    "cohere-ai": "6.0.0",
    "cross-env": "^7.0.3",
    "dts-bundle-generator": "^8.0.1",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-watch": "^5.0.1",
    "jest": "^29.5.0",
    "jimp": "0.16.2",
    "node-fetch": "2.6.12",
    "npm-run-all": "^4.1.5",
    "onnxruntime-node": "1.15.1",
    "onnxruntime-web": "^1.15.1",
    "openai": "^3.3.0",
    "openapi-generator-plus": "^2.6.0",
    "prettier": "2.8.7",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsd": "^0.28.1",
    "typescript": "^5.0.4"
  },
  "main": "./lib/node/index.js",
  "module": "./lib/esm/index.js",
  "scripts": {
    "test": "run-s db:clean db:run test:runfull db:clean db:run-auth test:runfull-authonly db:clean",
    "test:set-port": "cross-env URL=localhost:8001",
    "test:run": "jest --runInBand --testPathIgnorePatterns=test/auth.basic.test.ts",
    "test:run-auth": "jest --runInBand --testPathPattern=test/auth.basic.test.ts",
    "test:runfull": "PORT=8001 jest --runInBand --testPathIgnorePatterns=test/auth.basic.test.ts",
    "test:runfull-authonly": "PORT=8001 jest --runInBand --testPathPattern=test/auth.basic.test.ts",
    "test:update": "run-s db:clean db:run && jest --runInBand --updateSnapshot && run-s db:clean",
    "db:clean": "cd ../.. && CHROMA_PORT=8001 docker-compose -f docker-compose.test.yml down --volumes",
    "db:run": "cd ../.. && CHROMA_PORT=8001 docker-compose -f docker-compose.test.yml up --detach && sleep 5",
    "db:run-auth": "cd ../.. && CHROMA_PORT=8001 docker-compose -f docker-compose.test-auth.yml up --detach && sleep 5",
    "clean": "rimraf dist",
    "build": "node build.js",
    "genapi": "./genapi.sh",
    "prettier": "prettier --write .",
    "release": "run-s build test:run && npm publish",
    "prepublishOnly": "gulp check-package-json-files",
    "postpublish": "gulp build:cleanup"
  },
  "dependencies": {
    "isomorphic-fetch": "^3.0.0"
  },
  "peerDependencies": {
    "@visheratin/web-ai": "^1.0.0",
    "@visheratin/web-ai-node": "^1.0.0",
    "@xenova/transformers": "^2.0.0",
    "cohere-ai": "^6.0.0",
    "openai": "^3.0.0 | ^4.0.0"
  },
  "peerDependenciesMeta": {
    "@visheratin/web-ai": {
      "optional": true
    },
    "@visheratin/web-ai-node": {
      "optional": true
    },
    "@xenova/transformers": {
      "optional": true
    },
    "cohere-ai": {
      "optional": true
    },
    "openai": {
      "optional": true
    }
  },
  "exports": {
    ".": {
      "types": "./lib/node/index.d.ts",
      "require": "./lib/node/index.js",
      "import": "./lib/esm/index.js"
    },
    "./lib/*": {
      "default": "./lib/*"
    },
    "./cohere": {
      "types": "./cohere.d.ts",
      "require": "./lib/node/embeddings/cohere/CohereEmbeddingFunction.js",
      "import": "./lib/esm/embeddings/cohere/CohereEmbeddingFunction.js"
    },
    "./openai": {
      "types": "./openai.d.ts",
      "require": "./lib/node/embeddings/openai/OpenAIEmbeddingFunction.js",
      "import": "./lib/esm/embeddings/openai/OpenAIEmbeddingFunction.js"
    },
    "./transformers": {
      "types": "./transformers.d.ts",
      "require": "./lib/node/embeddings/transformers/TransformersEmbeddingFunction.js",
      "import": "./lib/esm/embeddings/transformers/TransformersEmbeddingFunction.js"
    },
    "./webai": {
      "types": "./webai.d.ts",
      "require": "./lib/node/embeddings/webai/WebAIEmbeddingFunction.js",
      "import": "./lib/esm/embeddings/webai/WebAIEmbeddingFunction.js"
    }
  },
  "files": [
    "lib",
    "LICENSE",
    "README.md",
    "cohere.js",
    "openai.js",
    "transformers.js",
    "webai.js",
    "cohere.d.ts",
    "openai.d.ts",
    "transformers.d.ts",
    "webai.d.ts"
  ]
}
