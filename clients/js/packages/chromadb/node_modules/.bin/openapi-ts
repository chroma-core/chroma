#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/itaismith/Developer/chroma/clients/js/node_modules/.pnpm/@hey-api+openapi-ts@0.67.3_typescript@5.8.3/node_modules/@hey-api/openapi-ts/bin/node_modules:/Users/itaismith/Developer/chroma/clients/js/node_modules/.pnpm/@hey-api+openapi-ts@0.67.3_typescript@5.8.3/node_modules/@hey-api/openapi-ts/node_modules:/Users/itaismith/Developer/chroma/clients/js/node_modules/.pnpm/@hey-api+openapi-ts@0.67.3_typescript@5.8.3/node_modules/@hey-api/node_modules:/Users/itaismith/Developer/chroma/clients/js/node_modules/.pnpm/@hey-api+openapi-ts@0.67.3_typescript@5.8.3/node_modules:/Users/itaismith/Developer/chroma/clients/js/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/itaismith/Developer/chroma/clients/js/node_modules/.pnpm/@hey-api+openapi-ts@0.67.3_typescript@5.8.3/node_modules/@hey-api/openapi-ts/bin/node_modules:/Users/itaismith/Developer/chroma/clients/js/node_modules/.pnpm/@hey-api+openapi-ts@0.67.3_typescript@5.8.3/node_modules/@hey-api/openapi-ts/node_modules:/Users/itaismith/Developer/chroma/clients/js/node_modules/.pnpm/@hey-api+openapi-ts@0.67.3_typescript@5.8.3/node_modules/@hey-api/node_modules:/Users/itaismith/Developer/chroma/clients/js/node_modules/.pnpm/@hey-api+openapi-ts@0.67.3_typescript@5.8.3/node_modules:/Users/itaismith/Developer/chroma/clients/js/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@hey-api/openapi-ts/bin/index.cjs" "$@"
else
  exec node  "$basedir/../@hey-api/openapi-ts/bin/index.cjs" "$@"
fi
