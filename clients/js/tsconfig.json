{
  
  "files": [
    "src/index.ts",
    "src/generated/index.ts",
    "src/embeddings/**/*.ts",
  ],
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "module": "CommonJS",
    "outDir": "dist/main",
    "sourceMap": true,
    "target": "ES2017",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "forceConsistentCasingInFileNames": true,
    "stripInternal": true,
    "isolatedModules": true,
  }
}
